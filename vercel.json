{
    "version": 2,
    "builds": [
      {
        "src": "api/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/products",
        "dest": "api/productsRouter.js"  
      },
      {
        "src": "/(.*)",
        "dest": "api/app.js"
      }
    ]
  }
  